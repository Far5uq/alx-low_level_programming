#include "main.h"
#include "dog.h"

/**
 * init_dog - initiate variable types
 * @d: structure
 * @name: pointer 1
 * @age: floater
 * @owner: pointer 2
 *
 * Return: Always 0.
 */

void init_dog(struct dog *d, char *name, float age, char *owner)
{
if (d)
{
d->name = name;
d->age = age;
d->owner = owner;
}
}
