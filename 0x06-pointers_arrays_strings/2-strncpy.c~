/**
 * *_strncpy - function that copies a code
 * @*dest: pointer 1
 * @*src: pointer 2
 * @n: pointer 3
 *
 * Return: Always 0.
 */

char *_strncpy(char *dest, char *src, int n)
{
int c;
for (c = 0 ; c < n && src[c] != '\0' ; c++)
dest[c] = src[c];
while (i < n)
{
dest[c] = '\0';
c++;
}
return (dest);
}
